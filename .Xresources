!*faceName: Terminus
!*faceSize: 10
!*font: xft:Envypn:13

XTerm*saveLines: 10000

urxvt.font: xft:Sauce Code Powerline:size=9:antialias=false
urxvt.depth: 32
urxvt.scrollBar: false
urxvt.secondaryScreen: 1
urxvt.secondaryScroll: 0
urxvt.letterSpace: -2

urxvt.perl-ext-common: default,matcher
urxvt.url-launcher: /usr/bin/chromium
urxvt.match.button: 1

*cursorBlink: true
*cursorColor: #ebdbb2

urxvt.background: #282828
urxvt.transparent: false
urxvt.shading: 30
urxvt.blurRadius: 5
urxvt.fading: 20

! hard contrast: *background: #1d2021
*background: #282828
! soft contrast: *background: #32302f
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #fb4934
*color9:  #cc241d
! DarkGreen + Green
*color2:  #b8bb26
*color10: #98971a
! DarkYellow + Yellow
*color3:  #fabd2f
*color11: #d79921
! DarkBlue + Blue
*color4:  #83a598
*color12: #458588
! DarkMagenta + Magenta
*color5:  #d3869b
*color13: #b16286
! DarkCyan + Cyan
*color6:  #8ec07c
*color14: #689d6a
! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2

! vim: set ft=xdefaults:
