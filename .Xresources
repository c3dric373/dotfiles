XTerm*saveLines: 10000

urxvt.font: xft:Source Code Pro:size=12,xft:Noto Emoji
urxvt.depth: 32
urxvt.scrollBar: false
urxvt.secondaryScreen: 1
urxvt.secondaryScroll: 0
urxvt.letterSpace: -1

urxvt.perl-ext-common: default,matcher
urxvt.url-launcher: /usr/bin/chromium
urxvt.match.button: 1

*cursorBlink: true

! special
*.foreground:   #f5ebe1
*.background:   #242424
*cursorColor:	#f5ebe1

! black
*.color0:       #1c1818
*.color8:       #070302

! red
*.color1:       #ff5959
*.color9:       #b52a48

! green
*.color2:       #91c55d
*.color10:      #5e9c4a

! yellow
*.color3:       #eaa835
*.color11:      #bfb96a

! blue
*.color4:       #1aadc3
*.color12:      #408cf3

! magenta
*.color5:       #f55cf8
*.color13:      #563680

! cyan
*.color6:       #419651
*.color14:      #216775

! white
*.color7:       #fdf2e5
*.color15:      #838383

urxvt.colorIT:	#ff5959
urxvt.colorBD:	#eaa835
urxvt.colorUL:	#f55cf8

!! vim: set ft=xdefaults:
